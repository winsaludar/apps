{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "TokenName": "APPS_AUTH_TOKEN",
    "RefreshTokenName": "APPS_REFRESH_TOKEN",
    "ContactUsEmail": ""
  },
  "AuthenticationApiSettings": {
    "BaseUrl": "services__authentication-api__https__0",
    "LoginRoute": "/api/auth/login",
    "RegisterRoute": "/api/auth/register",
    "ConfirmEmailRoute": "/api/auth/confirm-email",
    "ResendEmailConfirmationRoute": "/api/auth/resend-email-confirmation",
    "ForgotPasswordRoute": "/api/auth/forgot-password",
    "ResetPasswordRoute": "/api/auth/reset-password"
  }
}
